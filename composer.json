{
    "name": "pigoenphp/pigoenphp",
    "type": "project",
    "require": {
        "php": "^8.0",
        "phpmailer/phpmailer": "^6.9",
        "cboden/ratchet": "^0.4.4",
        "doctrine/dbal": "^4.2",
        "symfony/cache": "^7.1",
        "doctrine/orm": "^3.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "psr-0": {
          "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "CodeWithSushil",
            "email": "sushilkumbhar77@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.5"
    }
}
